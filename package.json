    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "susy": "^3.0.1",
    "typescript": "^2.4.2",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack": "^3.6.0",
    "webpack-plugin-critical": "^1.0.0"
  },
  "pre-commit": [
    "lint"
  ]
}
